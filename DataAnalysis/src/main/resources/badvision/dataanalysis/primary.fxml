<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="508.0" prefWidth="880.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="badvision.dataanalysis.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Button mnemonicParsing="false" onAction="#loadDataFiles" text="Load data files" />
      <TableView fx:id="subjectTable">
        <columns>
          <TableColumn fx:id="fileColumn" prefWidth="170" text="File" />
          <TableColumn fx:id="ipColumn" prefWidth="120" text="IP" />
            <TableColumn fx:id="locationColumn" minWidth="0" prefWidth="130" text="Location" />
            <TableColumn fx:id="numRecordsColumn" minWidth="0.0" prefWidth="60" text="# Records" />
            <TableColumn fx:id="experimentStartColumn" prefWidth="150" text="Start" />
            <TableColumn fx:id="experimentEndColumn" prefWidth="150" text="End" />
            <TableColumn fx:id="factorOrderColumn" prefWidth="100" text="Factor Order" />
        </columns>
      </TableView>
      <TableView fx:id="experimentTable" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="trialDateColumn" prefWidth="75.0" text="Date" />
          <TableColumn fx:id="recordTypeColumn" prefWidth="75.0" text="Record Type" />
        </columns>
      </TableView>
   </children>
</VBox>
